<%-include("header") -%>
<div class="box" id="heading">
  <h1> <%=listTitle%> </h1>
</div>
<div class="box">
  <% newListItems.forEach((item)=>{ %>
  <form class="item" method="post" action="/delete"> 
    <div class="item">
    <input type="checkbox" name=<%=listTitle%> value=<%=item._id%> onchange="this.form.submit()"/>
    <p> <%= item.name %> </p>
  </div>
</form>
  <% }) %>
  <form class="item form-two" method="post" action="/">
    <input
      type="text"
      name="newItem"
      placeholder="New Item"
      autocomplete="off"
      required
    />
    <button type="submit" name="list" value= <%=listTitle%> >+</button>
  </form>
</div>
<%-include("footer") -%>
